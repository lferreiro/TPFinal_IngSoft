<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id = "helloWorld" class = "Crimson.Crimson_core.HelloWorld">
        <property name = "message" value = "Hello World!"/>
    </bean>
    <bean id="crimsonController" class="Crimson.Crimson_core.RestApp.CrimsonController"/>
    <bean id="cartelera" class="Crimson.Crimson_core.Cartelera"/>
    <bean id="dataLoader" class="Crimson.Crimson_core.Dummys.DataLoader"/>
    <bean id="dataManager" class="Crimson.Crimson_core.RestApp.DataManager"/>
    <bean id="intermodelo" class="Crimson.Crimson_core.RestApp.Intermodelo"/>

</beans>
