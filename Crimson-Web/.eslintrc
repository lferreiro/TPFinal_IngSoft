{
  "env": {
    "browser": true, 
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "import/extensions": "off",
    "jsx-a11y/anchor-is-valid": ["error", { "components": ["Link"], "specialLink": ["to"] }],
    "jsx-a11y/label-has-for": "off",
    "react/prefer-stateless-function": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}
